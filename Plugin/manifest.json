{
  "manifest_version": 2,
  "name": "Security plugin",
  "description": "Check for webpages that uses user's cpu resource maliciously",
  "version": "1.0",
  "background":{
    "scripts":["background.js"],
    "persistent":true
  },
  "icons":{
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "<all_urls>",
    "webRequestBlocking"
  ]
}
